				#elif defined(MAIN_LIGHT_CALCULATE_SHADOWS)
					ShadowCoords = TransformWorldToShadowCoord( WorldPosition );
				#endif
	
				WorldViewDirection = SafeNormalize( WorldViewDirection );

				float2 texCoord31 = IN.ase_texcoord7.xy * float2( 0,10 ) + float2( 0,0.17 );
				float2 uv_WarpTexture = IN.ase_texcoord7.xy * _WarpTexture_ST.xy + _WarpTexture_ST.zw;
				float2 lerpResult30 = lerp( float2( 0,0 ) , (tex2D( _WarpTexture, uv_WarpTexture )).rg , _WarpIntensity);
				float2 temp_output_37_0 = ( texCoord31 + lerpResult30 );
				float4 lerpResult15 = lerp( _BaseColor01 , _BaseColor02 , (tex2D( _TextureSample0, temp_output_37_0 )).r);
				float4 temp_output_43_0 = ( lerpResult15 * _BoardsBrightness );
				float4 lerpResult13 = lerp( temp_output_43_0 , ( temp_output_43_0 * ( 1.0 - saturate( _BoardsContrast ) ) ) , IN.ase_color.r);
				float temp_output_1_0_g1 = _Contrast;
				float2 texCoord22 = IN.ase_texcoord7.xy * float2( 12,12 ) + float2( 0,0 );
				float lerpResult5_g1 = lerp( ( 0.0 - temp_output_1_0_g1 ) , ( temp_output_1_0_g1 + 1.0 ) , ( ( tex2D( _WoodMask, texCoord22 ).r * IN.ase_color.g ) * 4.0 ));
				float4 lerpResult9 = lerp( lerpResult13 , _WoodChamfers , saturate( lerpResult5_g1 ));
				float4 lerpResult8 = lerp( lerpResult9 , _RopeColor , IN.ase_color.b);
				
